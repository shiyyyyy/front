/*弹窗关闭按钮*/
.closeBtn {
    position: absolute;
    top: 0;
    right: 0;
    display: block;
    width: 30px;
    height: 30px;
    opacity: 1;
    background: url(../img/close1.png) no-repeat center center!important;
    background-size: 100% 100%!important;
}

.closeBtn:hover,
.closeBtn:focus {
    background: url(../img/close2.png) no-repeat center center!important;
    background-size: 100% 100%!important;
    opacity: 1;
}

/*图标旋转一周*/
.header-tab-item .del-btn i {
  display: inline-block;
}
.header-tab-item .del-btn:hover i {
  -webkit-animation: rotate 0.3s linear 0s normal;
  animation: rotate 0.3s linear 0s normal;
}
@-webkit-keyframes rotate {
  from {
    -webkit-transform: rotate(0deg)
  }
  to {
    -webkit-transform: rotate(180deg)
  }
}
@keyframes rotate {
  from {
    transform: rotate(0deg)
  }
  to {
    transform: rotate(180deg)
  }
}


