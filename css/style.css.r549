/*首页*/
.shadow {
  box-shadow: 0 0 10px rgba(0,0,0,0.2);
}
.bottomInfo {
  background: #fff;
}
.topTitle {
  padding: 12px;
  background: #f0f0f0;
}
.titleLeft {
  float: left;
  line-height: 23px;
}
.titleLeft span {
  padding-left: 2px;
}
.titleRight {
  float: right;
  font-size: 12px;
}
.titleRight button {
  padding: 3px 8px;  
  border: none;
  border-radius: 4px;
  transition: all 0.3s;   
}
.titleRight button:hover { 
    color: #fff;  
    background: #3293da;
    box-shadow: 0 0 5px rgba(123,219,243,0.5);
}
.titleRight i {
  display: block;
  transform: scale(1.2);
}
.personImg {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 3;
}
.personImg img {
  width: 80px;
  height: 80px;
}
.personInfo {
  display: flex;
  flex: 7;
}
.personInfo ul {
  list-style: none;
  margin: 25px 0 0 0;
  padding: 0;
}
.personInfo li {
  padding-bottom: 10px;
  font-size: 12px;
}
.infoBtn {
  display: flex;
  padding: 10px 15px 15px 15px;
}
.infoBtn a {
  flex: 1;
  padding: 2px 3px;
  font-size: 10px;
  color: #3293da;
  text-align: center;
  border: 1px solid #3293da;
  border-radius: 4px;
}
.infoBtn a:hover {
  color: #fff;
  background: #3293da;
}
.infoBtn a:not(:last-child) {
  margin-right: 8px;  
}
.fc-toolbar.fc-header-toolbar {
  margin-bottom: 0!important;
}

.fc-toolbar .fc-left,
.fc-toolbar .fc-right {
    padding: 12px;
}
.fc-toolbar .fc-left h2 {
  font-size: 24px;
}


