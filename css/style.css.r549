.pace {
  -webkit-pointer-events: none;
  pointer-events: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none;
}

.pace-inactive,
.tooltip.gridTip .tooltip-arrow,
.pvtRenderer,
.pvtVals,
.pvtAxisContainer {
  display: none;
}

.pace .pace-progress {
  background: #29d;
  position: fixed;
  z-index: 2000;
  top: 0;
  right: 100%;
  width: 100%;
  height: 2px;
}

.mask {
    position: fixed;
    top: 2px;
    left: 0;
    right: 0;
    bottom: 0;
    filter: alpha(opacity=30);
    background-color: #777;
    opacity: 0.3;
    -moz-opacity: 0.3;
    z-index: 1026;
}

.scroll-ng-text .ta-editor.ta-html, .scroll-ng-text .ta-scroll-window.form-control {
    max-height: 400px;
}
.tooltip.gridTip .tooltip-inner {
  color: #880000;
  background-color: #ffff66;
  box-shadow: 0 6px 12px rgba(0,0,0,.175);
}


.ui-grid-menu-button .ui-grid-menu .ui-grid-menu-mid {
  overflow: visible !important;
}
.ui-grid-pager-panel {
